	<div id="categories_strip">
		<ul>
			<li class="current_page_item home"><%= link_to "Home", root_path %></li>
			<% if signed_in? %>
				<li class="cat-item cat-item-2"><%= link_to "Users", users_path %></li>
				<li class="cat-item cat-item-3"><%= link_to "Events", events_path %></li>
				<li class="cat-item cat-item-4"><%= link_to "Profile", current_user %></li>
				<li class="cat-item cat-item-5"><%= link_to "Settings", edit_user_path(current_user )%></li>
			<% end %>
			<% if signed_in? %>
				<li class="cat-item cat-item-6"><%= link_to "Sign out", signout_path, :method => :delete %></li>
			<% else %>
				<li class="cat-item cat-item-7"><%= link_to "Sign in", signin_path %></li>
			<% end %>
		</ul>
	</div>